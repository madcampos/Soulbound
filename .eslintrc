{
	"ecmaVersion": 6,
	"rules": {
		"comma-dangle": [1, "never"],
		"no-cond-assign": [2, "except-parens"],
		"no-console": 0,
		"no-constant-condition": 2,
		"no-control-regex": 1,
		"no-debugger": 1,
		"no-dupe-args": 2,
		"no-dupe-keys": 1,
		"no-duplicate-case": 2,
		"no-empty-character-class": 2,
		"no-empty": 1,
		"no-ex-assign": 2,
		"no-extra-boolean-cast": 2,
		"no-extra-parens": [1, "all"],
		"no-extra-semi": 2,
		"no-func-assign": 2,
		"no-inner-declarations": [1, "both"],
		"no-invalid-regexp": 2,
		"no-irregular-whitespace": 2,
		"no-negated-in-lhs": 1,
		"no-obj-calls": 2,
		"no-regex-spaces": 2,
		"no-sparse-arrays": 1,
		"no-unexpected-multiline": 2,
		"no-unreachable": 1,
		"use-isnan": 2,
		"valid-jsdoc": [1, {"prefer": {"return": "return"}, "requireReturn": false, "requireReturnDescription": true, "requireParamDescription": true, "requireReturnType": true}],
		"valid-typeof": 1,
		"accessor-pairs": 1,
		"complexity": 1,
		"consistent-return": 1,
		"curly": [2, "multi-line", "consistent"],
		"dot-location": [2, "property"],
		"dot-notation": [2, {"allowPattern": "^[a-z]+(_[a-z]+)+$"}],
		"eqeqeq": [2, "smart"],
		"guard-for-in": 1,
		"no-alert": 2,
		"no-caller": 2,
		"no-case-declarations": 2,
		"no-div-regex": 2,
		"no-else-return": 2,
		"no-empty-pattern": 1,
		"no-eq-null": 2,
		"no-eval": 1,
		"no-extend-native": 1,
		"no-extra-bind": 2,
		"no-fallthrough": 2,
		"no-floating-decimal": 2,
		"no-implicit-coercion": [2, {"boolean": false}],
		"no-implied-eval": 2,
		"no-invalid-this": 2,
		"no-iterator": 2,
		"no-labels": 1,
		"no-lone-blocks": 2,
		"no-loop-func": 2,
		"no-magic-numbers": 0,
		"no-multi-spaces": 2,
		"no-multi-str": 2,
		"no-native-reassign": 2,
		"no-new-func": 1,
		"no-new-wrappers": 2,
		"no-new": 2,
		"no-octal-escape": 2,
		"no-octal": 2,
		"no-param-reassign": [2, {"props": true}],
		"no-process-env": 1,
		"no-proto": 2,
		"no-redeclare": [2, {"builtinGlobals": true}],
		"no-return-assign": 2,
		"no-script-url": 2,
		"no-self-compare": 2,
		"no-sequences": 2,
		"no-throw-literal": 2,
		"no-unused-expressions": [2, {"allowShortCircuit": true, "allowTernary": true}],
		"no-useless-call": 2,
		"no-useless-concat": 2,
		"no-void": 2,
		"no-with": 2,
		"radix": [2, "always"],
		"vars-on-top": 1,
		"wrap-iife": [2, "inside"],
		"yoda": 2,
		"strict": [1, "global"],
		"no-catch-shadow": 2,
		"no-delete-var": 2,
		"no-label-var": 2,
		"no-shadow-restricted-names": 2,
		"no-shadow": 1,
		"no-undef-init": 2,
		"no-undef": [2, {"typeof": true}],
		"no-undefined": 2,
		"no-unused-vars": [1, {"args": "all"}],
		"no-use-before-define": 2,
		"global-require": 1,
		"handle-callback-err": [1, "^.*(e|E)rr"],
		"no-mixed-requires": 1,
		"no-path-concat": 2,
		"no-process-exit": 1,
		"array-bracket-spacing": 1,
		"block-spacing": [1, "never"],
		"brace-style": [1, "1tbs", {"allowSingleLine": true}],
		"camelcase": 1,
		"comma-spacing": [1, {"before": false, "after": true}],
		"comma-style": [1, "last"],
		"computed-property-spacing": [1, "never"],
		"consistent-this": [1, "self"],
		"func-style": [1, "declaration"],
		"id-length": [1, {"min": 3, "max": 25, "properties": "never", "exceptions": ["i", "j", "n", "w", "x", "y", "z", "id", "fs"]}],
		"indent": [2, "tab", {"SwitchCase": 1}],
		"key-spacing": 2,
		"max-depth": [1, 5],
		"max-nested-callbacks": [1, 5],
		"max-statements": [1, 150],
		"new-cap": [2, {"capIsNew": false, "newIsCap": true}],
		"new-parens": 2,
		"no-array-constructor": 2,
		"no-bitwise": 2,
		"no-continue": 2,
		"no-lonely-if": 0,
		"no-mixed-spaces-and-tabs": 2,
		"no-multiple-empty-lines": [2, {"max": 2, "maxEOF": 0}],
		"no-negated-condition": 0,
		"no-nested-ternary": 2,
		"no-spaced-func": 2,
		"no-trailing-spaces": 2,
		"no-underscore-dangle": 2,
		"no-unneeded-ternary": 2,
		"object-curly-spacing": [2, "never"],
		"operator-linebreak": [2, "before", {"overrides": {"?": "after"}}],
		"linebreak-style": [1, "unix"],
		"quote-props": [1, "as-needed", {"keywords": true, "numbers": true}],
		"quotes": [2, "single"],
		"require-jsdoc": 2,
		"semi-spacing": [2, {"before": false, "after": true}],
		"semi": 2,
		"keyword-spacing": [2, {"before": true, "after": true, "overrides": {"function": {"after": false}}}],
		"space-before-blocks": [1, {"functions": "never", "keywords": "always"}],
		"space-before-function-paren": [2, { "anonymous": "never", "named": "never" }],
		"space-in-parens": [2, "never"],
		"space-infix-ops": 2,
		"space-unary-ops": [2, { "words": true, "nonwords": false }],
		"wrap-regex": 2,
		"arrow-parens": [2, "always"],
		"arrow-spacing": 2,
		"constructor-super": 2,
		"generator-star-spacing": [2, {"before": true, "after": false}],
		"yield-star-spacing": [2, {"before": true, "after": false}],
		"no-confusing-arrow": 1,
		"no-class-assign": 2,
		"no-const-assign": 2,
		"no-dupe-class-members": 2,
		"no-this-before-super": 2,
		"no-var": 1,
		"prefer-const": 0,
		"prefer-reflect": 1,
		"prefer-spread": 1,
		"prefer-template": 1,
		"require-yield": 2,
		"array-callback-return": 2,
		"no-empty-function": 1,
		"no-implicit-globals": 1,
		"no-new-symbol": 2,
		"no-self-assign": 2,
		"no-useless-constructor": 2,
		"no-whitespace-before-property": 2,
		"prefer-rest-params": 1,
		"template-curly-spacing": [2, "never"]
	},
	"env": {
		"shared-node-browser": true,
		"worker": true,
		"es6": true,
		"serviceworker": true,
		"node": true
	}
}