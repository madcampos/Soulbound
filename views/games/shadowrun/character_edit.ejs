	<!-- Picture -->

	<article id="picture">
		<h2><%=__('Picture')%></h2>
		<% if(hasAvatar(char._id)){ %>
			<div id="pictureContainer">
				<div id="pictureWrapper">
					<%-'<img src="/images/public/characters/' + char._id + '.png" alt="' + __('Avatar of: ') + char.name + '">'%>
				</div>
			</div>
		<% } else { %>
			<div id="emptyPictureContainer">
				<span><%=__('Drop Picture here or...')%></span>
				<input type="file">
				<span><%=__('Open the file.')%></span>
			</div>
		<% } %>
	</article>
	
	<!-- Qualities -->

	<article id="qualities">
		<h2><%=__('Qualities')%></h2>
		<div class="containers">
			<table>
				<tr>
					<th><%=__('Name:')%></th>
				</tr>
				<% for (var quality in char.qualities) { %>
					<tr>
						<td>
							<select>
								<% for (var i in game.qualityTypes) { %>
									<%-'<optgroup label="' + __(game.qualityTypes[i][0]) + '" title="' + __(game.qualityTypes[i][1]) + '">'%>
										<% for (var j in game.qualityList[i]){
											if (game.qualityList[i][j].length == 3) {
												if (char.qualities[quality] == game.qualityList[i][j][0]) { %>
													<%-'<option selected="selected" class="' + game.qualityList[i][j][2] + '" title="' + __(game.qualityList[i][j][1]) + '">' + __(game.qualityList[i][j][0]) + '</option>'%>
												<% } else { %>
													<%-'<option class="' + game.qualityList[i][j][2] + '" title="' + __(game.qualityList[i][j][1]) + '">' + __(game.qualityList[i][j][0]) + '</option>'%>
												<% }
											} else {
												if (char.qualities[quality] == game.qualityList[i][j][0]) { %>
													<%-'<option selected="selected" title="' + __(game.qualityList[i][j][1]) + '">' + __(game.qualityList[i][j][0]) + '</option>'%>
												<% } else { %>
													<%-'<option title="' + __(game.qualityList[i][j][1]) + '">' + __(game.qualityList[i][j][0]) + '</option>'%>
												<% }
											}
										} %>
									</optgroup>
								<% } %>
							</select>
						</td>
					</tr>
				<% } %>
			</table>
		</div>
		<div class="actionButtons">
			<%-'<input type="button" value="' + __('Add Quality') + '">'%>
			<%-'<input type="button" value="' + __('Remove Quality') + '">'%>
		</div>
	</article>
	
	<!-- Melee Weapons -->
	
	<article id="weapons">
		<h2><%=__('Melee Weapons')%></h2>
		<div class="containers">
			<table>
				<tr>
					<th><%=__('Weapon:')%></th>
					<th><%=__('Reach:')%></th>
					<th><%=__('Damage:')%></th>
					<%-'<th title="' + __('Armor Penetration') + '">' + __('AP:') + '</th>'%>
				</tr>
				<% for (var weapon in char.weapons) { %>
					<tr>
						<td>
							<select>
								<% for (var i in game.weaponTypes) { %>
									<%-'<optgroup label="' + __(game.weaponTypes[i][0]) + '" title="' + __(game.weaponTypes[i][1]) + '">'%>
										<% for (var j in game.weaponList[i]){
											if (game.weaponList[i][j].length == 3) {
												if (char.weapons[weapon] == game.weaponList[i][j][0]) { %>
													<%-'<option selected="selected" class="' + game.weaponList[i][j][2] + '" title="' + __(game.weaponList[i][j][1]) + '">' + __(game.weaponList[i][j][0]) + '</option>'%>
												<% } else { %>
													<%-'<option class="' + game.weaponList[i][j][2] + '" title="' + __(game.weaponList[i][j][1]) + '">' + __(game.weaponList[i][j][0]) + '</option>'%>
												<% }
											} else {
												if (char.weapons[weapon] == game.weaponList[i][j][0]) { %>
													<%-'<option selected="selected" title="' + __(game.weaponList[i][j][1]) + '">' + __(game.weaponList[i][j][0]) + '</option>'%>
												<% } else { %>
													<%-'<option title="' + __(game.weaponList[i][j][1]) + '">' + __(game.weaponList[i][j][0]) + '</option>'%>
												<% }
											}
										} %>
									</optgroup>
								<% } %>
							</select>
						</td>
						<td><%-'<input type="number" value="' + char.weapons[weapon][1] + '">'%></td>
						<td><%-'<input type="number" value="' + char.weapons[weapon][2] + '">'%></td>
						<td><%-'<input type="number" value="' + char.weapons[weapon][3] + '">'%></td>
					</tr>
				<% } %>
			</table>
		</div>
		<div class="actionButtons">
			<%-'<input type="button" value="' + __('Add Weapon') + '">'%>
			<%-'<input type="button" value="' + __('Remove Weapon') + '">'%>
		</div>
	</article>
	
	<!-- Ranged Weapons -->

	<article id="guns">
		<h2><%=__('Ranged Weapons')%></h2>
		<div class="containers">
			<table>
				<tr>
					<th><%=__('Gun:')%></th>
					<th><%=__('Damage:')%></th>
					<%-'<th title="' + __('Armor Penetration') + '">' + __('AP:') + '</th>'%>
					<%-'<th title="' + __('SS = Single Shot&#10;SA = Semi-Automatic&#10;BF = Burst Fire&#10;FA = Full-Automatic') + '">' + __('Mode:') + '</th>'%>
					<%-'<th title="' + __('Recoil Compensation') + '">' + __('RC:') + '</th>'%>
					<th><%=__('Ammo:')%></th>
				</tr>
				<% for (var gun in char.guns) { %>
					<tr>
						<td>
							<select>
								<% for (var i in game.gunsGroups) { %>
									<%-'<optgroup label="' + __(game.gunsGroups[i][0]) + '" title="' + __(game.gunsGroups[i][1]) + '">'%>
										<% for (var j in game.gunsList[i]){
											if (game.gunsList[i][j].length == 3) {
												if (char.guns[gun][0] == game.gunsList[i][j][0]) { %>
													<%-'<option selected="selected" class="' + game.gunsList[i][j][2] + '" title="' + __(game.gunsList[i][j][1]) + '">' + __(game.gunsList[i][j][0]) + '</option>'%>
												<% } else { %>
													<%-'<option class="' + game.gunsList[i][j][2] + '" title="' + __(game.gunsList[i][j][1]) + '">' + __(game.gunsList[i][j][0]) + '</option>'%>
												<% }
											} else {
												if (char.guns[gun][0] == game.gunsList[i][j][0]) { %>
													<%-'<option selected="selected" title="' + __(game.gunsList[i][j][1]) + '">' + __(game.gunsList[i][j][0]) + '</option>'%>
												<% } else { %>
													<%-'<option title="' + __(game.gunsList[i][j][1]) + '">' + __(game.gunsList[i][j][0]) + '</option>'%>
												<% }
											}
										} %>
									</optgroup>
								<% } %>
							</select>
						</td>
						<td><%-'<input type="number" value="' + char.guns[gun][1] + '">'%></td>
						<td><%-'<input type="number" value="' + char.guns[gun][2] + '">'%></td>
						<td>
							<select>
								<% var mode = ['SS','SA','BF','FA'];
								for (var i in mode) {
									if (char.guns[gun][3] == mode[i]){ %>
										<option selected="selected"><%=mode[i]%></option>
									<% } else { %>
										<option><%=mode[i]%></option>
									<% }
								} %>
							</select>
						</td>
						<td><%-'<input type="number" value="' + char.guns[gun][4] + '">'%></td>
						<td><%-'<input type="number" value="' + char.guns[gun][5] + '">'%></td>
					</tr>
				<% } %>
			</table>
		</div>
		<div class="actionButtons">
			<%-'<input type="button" value="' + __('Add Gun') + '">'%>
			<%-'<input type="button" value="' + __('Remove Gun') + '">'%>
		</div>
	</article>
	
	<!-- Armor -->

	<article id="armor">
		<h2><%=__('Armor')%></h2>
		<div class="containers">
			<table>
				<tr>
					<th><%=__('Armor (Balistic/Impact):')%></th>
				</tr>
				<% for (var armor in char.armors) {%>
					<tr>
						<td>
							<select>
								<% for (var i in game.armorGroups) { %>
									<%-'<optgroup label="' + __(game.armorGroups[i][0]) + '" title="' + __(game.armorGroups[i][1]) + '">'%>
										<% for (var j in game.armorList[i]){
