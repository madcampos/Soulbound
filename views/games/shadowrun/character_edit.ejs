<!-- sheet -->
<script type="text/javascript">
	var msg = [<%-'\'' + __('Please, drop an image.') + '\''%>,<%-'\'' + __('Image size must be less than 45kb.') + '\''%>];
	var game =  <%-JSON.stringify(game)%>;
</script>
<div id="xhrMsg" style="display:none;"></div>
<div id="sheet">
	<article id="personalData">
		<h2><%=__('Personal Data')%></h2>
		<table>
			<tr>
				<td>
					<label for="name"><%=__('Name:')%></label>
				</td>
				<td colspan="8">
					<%-'<input type="text" id="name" value="' + char.name + '">'%>
				</td>
			</tr>
			<tr>
				<td>
					<label for="metatype"><%=__('Metatype:')%></label>
				</td>
				<td>
					<select id="metatype">
						<% for (var i in game.metatypes) {
							if (game.metatypes[i][0] == char.metatype) { %>
								<%-'<option selected="selected" title="' + __(game.metatypes[i][2]) + '">' + __(game.metatypes[i][0]) + '</option>'%>
							<% } else { %>
								<%-'<option title="' + __(game.metatypes[i][2]) + '">' + __(game.metatypes[i][0]) + '</option>'%>
							<% }
						} %>
					</select>
				</td>
				<td>
					<label for="ethnicity"><%=__('Ethnicity:')%></label>
				</td>
				<td colspan="2">
					<%-'<input type="text" id="ethnicity" value="' + char.ethinicity + '">'%>
				</td>
				<td>
					<label for="age"><%=__('Age:')%></label>
				</td>
				<td>
					<%-'<input type="date" id="age" value="' + char.age + '">'%>
				</td>
				<td>
					<label for="sex"><%=__('Sex:')%></label>
				</td>
				<td>
					<%-'<input type="text" id="sex" value="' + char.sex + '">'%>
				</td>
			</tr>
			<tr>
				<td>
					<label for="totalKarma"><%=__('Karma (Total/Curr.):')%></label>
				</td>
				<td>
					<%-'<input type="number" min="0" id="totalKarma" value="' + char.karma + '">'%>
				</td>
				<td>
					<%-'<input type="number" min="0" id="currentKarma" value="' + char.currentKarma + '">'%>
				</td>
				<td>
					<label for="streetCredit"><%=__('Street Credit:')%></label>
				</td>
				<td>
					<input type="number" min="0" id="streetCredit">
				</td>
				<td>
					<label for="notoriety"><%=__('Notoriety:')%></label>
				</td>
				<td>
					<input type="number" min="0" id="notoriety">
				</td>
				<td>
					<label for="publicAwarness"><%=__('Public Aware.:')%></label>
				</td>
				<td>
					<input type="number" min="0" id="publicAwarness">
				</td>
			</tr>
		</table>
	</article>
	
	<!-- Attributes -->
	<article id="attributes">
		<h2><%=__('Attributes')%></h2>
		<table>
			<tr>
				<th colspan="2"><%=__('Phisical')%></th>
				<th colspan="2"><%=__('Mental')%></th>
				<th colspan="5"><%=__('Special')%></th>
			</tr>
			<tr>
				<td>
					<label for="body"><%=__('Body:')%></label>
				</td>
				<td>
					<%-'<input type="number" min="1" max="9" value="' + char.body + '" id="body" onmouseup="phisicalBoxes()">'%>
				</td>
				<td>
					<label for="charisma"><%=__('Charisma:')%></label>
				</td>
				<td>
					<%-'<input type="number" min="1" max="9" value="' + char.charisma + '" id="charisma">'%>
				</td>
				<td>
					<label for="edge"><%=__('Edge:')%></label>
				</td>
				<td colspan="2">
					<%-'<input type="number" min="1" max="9" value="' + char.edge + '" id="edge">'%>
				</td>
				<td colspan="2">
					<%-'<input type="number" min="1" max="9" value="' + char.currentEdge + '" id="currentEdge">'%>
				</td>
			</tr>
			<tr>
				<td>
					<label for="agility"><%=__('Agility:')%></label>
				</td>
				<td>
					<%-'<input type="number" min="1" max="9" value="' + char.agility + '" id="agility">'%>
				</td>
				<td>
					<label for="intuition"><%=__('Intuition:')%></label>
				</td>
				<td>
					<%-'<input type="number" min="1" max="9" value="' + char.intuition + '" id="intuition">'%>
				</td>
				<td>
					<label for="essence"><%=__('Essence:')%></label>
				</td>
				<td colspan="4">
					<%-'<input type="number" min="1" max="6" value="' + char.essence + '" step="0.1" id="essence">'%>
				</td>
			</tr>
			<tr>
				<td>
					<label for="reaction"><%=__('Reaction:')%></label>
				</td>
				<td>
					<%-'<input type="number" min="1" max="9" value="' + char.reaction + '" id="reaction">'%>
				</td>
				<td>
					<label for="logic"><%=__('Logic:')%></label>
				</td>
				<td>
					<%-'<input type="number" min="1" max="9" value="' + char.logic + '" id="logic">'%>
				</td>
				<td>
					<label for="magic" title="Magic/Ressonance"><%=__('Mag./Ress.:')%></label>
				</td>
				<td colspan="4">
					<%-'<input type="number" min="1" max="9" value="' + char.magic + '" id="magic">'%>
				</td>
			</tr>
			<tr>
				<td>
					<label for="strength"><%=__('Strength:')%></label>
				</td>
				<td>
					<%-'<input type="number" min="1" max="9" value="' + char.strength + '" id="strength">'%>
				</td>
				<td>
					<label for="willpower"><%=__('Willpower:')%></label>
				</td>
				<td>
					<%-'<input type="number" min="1" max="9" value="' + char.willpower + '" id="willpower" onmouseup="stunBoxes()">'%>
				</td>
				<td>
					<label for="initiative"><%=__('Initiative (Base/Astral/Matrix/Passes):')%></label>
				</td>
				<td>
					<%-'<input type="number" min="1" max="9" value="' + char.initiative + '" id="initiative">'%>
				</td>
				<td>
					<%-'<input type="number" min="1" max="18" value="' + char.matrixInit + '" id="matrixInit">'%>
				</td>
				<td>
					<%-'<input type="number" min="1" max="18" value="' + char.astralInit + '" id="astralInit">'%>
				</td>
				<td>
					<%-'<input type="number" min="1" max="4" value="' + char.initPasses + '" id="initPasses">'%>
				</td>
			</tr>
		</table>
	</article>
	

	<!-- Skills -->

	<article id="skills">
		<h2><%=__('Skills')%></h2>
		
		<!-- Active Skills Container -->
		
		<div>
			<div class="containers">
				<table id="activeSkills">
					<tr>
						<th><%=__('Name:')%></th> 
						<th><%=__('Rtg:')%></th>
					</tr>
					<% for (var activeSkill in char.activeSkills) { %>
						<tr>
							<td>
								<select>
									<% for (var i in game.attList) { %>
										<%-'<optgroup label="' + __(game.attList[i][0]) + '" title="' + __(game.attList[i][1]) + '">'%>
											<% for (var j in game.skillList[i]){
												if (game.skillList[i][j].length == 3) {
													if (char.activeSkills[activeSkill][0] == game.skillList[i][j][0]) { %>
														<%-'<option selected="selected" class="' + game.skillList[i][j][2] + '" title="' + __(game.skillList[i][j][1]) + '">' + __(game.skillList[i][j][0]) + '</option>'%>
													<% } else { %>
														<%-'<option class="' + game.skillList[i][j][2] + '" title="' + __(game.skillList[i][j][1]) + '">' + __(game.skillList[i][j][0]) + '</option>'%>
													<% }
												} else {
													if (char.activeSkills[activeSkill][0] == game.skillList[i][j]) { %>
														<%-'<option selected="selected" title="' + __(game.skillList[i][j][1]) + '">' + __(game.skillList[i][j][0]) + '</option>'%>
													<% } else { %>
														<%-'<option title="' + __(game.skillList[i][j][1]) + '">' + __(game.skillList[i][j][0]) + '</option>'%>
													<% }
												}
											} %>
										</optgroup>
									<% } %>
								</select>
							</td>
							<td><%-'<input type="number" value="' + char.activeSkills[activeSkill][1] + '">'%></td>
						</tr>
					<% } %>
				</table>
			</div>
			<div class="actionButtons">
				<%-'<input type="button" value="' + __('Add Skill') + '">'%>
				<%-'<input type="button" value="' + __('Remove Skill') + '">'%>
			</div>
		</div>
			
		<!-- Other Skills Container -->
		
		<div>
			<div class="containers">
				<table id="otherSkills">
					<tr>
						<th><%=__('Name:')%></th> 
						<th><%=__('Rtg:')%></th>
					</tr>
					<% for (var otherSkill in char.otherSkills) { %>
						<tr>
							<td><%-'<input type="text" value="' + char.otherSkills[otherSkill][0] + '">'%></td>
							<td><%-'<input type="number" value="' + char.otherSkills[otherSkill][1] + '">'%></td>
						</tr>
					<% } %>
				</table>
			</div>
			
			<div class="actionButtons">
				<%-'<input type="button" value="' + __('Add Skill') + '">'%>
				<%-'<input type="button" value="' + __('Remove Skill') + '">'%>
			</div>
		</div>
	</article>
	
